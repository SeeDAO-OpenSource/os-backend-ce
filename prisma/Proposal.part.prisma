model Proposal {
  id          Int      @id @default(autoincrement())
  proposer    User     @relation(fields: [proposerId], references: [id])
  proposerId  Int
  datetime    DateTime
  title       String
  content     String
  tags        String
  category    String
  links       String
  type        String
  status      String
  sip         String
  main_poll   String
  poll        String
  budget      Int
  down_payment Int
  subpolls    String
}